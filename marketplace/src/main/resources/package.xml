<package type="addon" name="nuxeo-web-mobile" version="@VERSION@">
  <title>Nuxeo Web Mobile</title>
  <description>
    <p>Nuxeo Web Mobile provides a mobile UI experience to allow user browsing documents through their smartphone.</p>
  </description>
  <home-page>https://github.com/nuxeo/nuxeo-web-mobile</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <require-terms-and-conditions-acceptance>true
  </require-terms-and-conditions-acceptance>
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>production_ready</production-state>
  <supported>true</supported>
  <platforms>
    <platform>cap-@DISTRIB_VERSION@</platform>
  </platforms>
  <license>LGPL 2.1</license>
  <license-url>http://www.gnu.org/licenses/lgpl-2.1.html</license-url>
  <visibility>PUBLIC</visibility>
</package>
